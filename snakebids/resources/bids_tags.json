    "chunk": {
        "tag": "chunk",
        "before": "[_/\\\\]+chunk-",
        "match": "[0-9]{1,5}"
    },
    "suffix": {
        "before": "(?:^|[_/\\\\])",
        "match": "[a-zA-Z0-9]+",
        "after": "\\.[^/\\\\]+$"
    },
    "datatype": {
        "before": "[/\\\\]+",
        "match": "anat|beh|dwi|eeg|fmap|func|ieeg|meg|micr|perf|pet",
        "after": "[/\\\\]+"
    },
    "extension": {
        "before": "[^./\\\\]",
        "match": "\\.[^/\\\\]+",
        "after": "$"
    },
    "split": {
        "tag": "split",
        "before": "[_/\\\\]+split-",
        "match": "[a-zA-Z0-9]+"
    },
    "atlas": {
